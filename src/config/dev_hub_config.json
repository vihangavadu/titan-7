{
  "ai": {
    "provider": "copilot",
    "api_key": "",
    "model": "gpt-4",
    "max_tokens": 2000,
    "temperature": 0.7,
    "endpoint": "https://api.githubcopilot.com"
  },
  "git": {
    "auto_commit": true,
    "commit_prefix": "[TITAN-DevHub]",
    "branch": "main",
    "auto_push": false
  },
  "ui": {
    "theme": "dark",
    "font_size": 12,
    "window_size": [1200, 800],
    "auto_save": true
  },
  "features": {
    "auto_suggest": true,
    "code_review": true,
    "task_tracking": true,
    "chat_history": true,
    "git_integration": true,
    "code_analysis": true
  },
  "titan": {
    "core_path": "/opt/titan/core",
    "modules_path": "/opt/titan/modules",
    "version": "7.6"
  }
}
